{
  "manifest_version": 2,

  "name": "No Vimeo Autoplay",
  "description": "This extensions prevents Vimeo videos from playing automatically",
  "icons": {
      "128": "icon128.png",
      "48": "icon48.png",
      "16": "icon16.png"
  },
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["script.js"],
      "run_at": "document_end"
    }
  ]
}
